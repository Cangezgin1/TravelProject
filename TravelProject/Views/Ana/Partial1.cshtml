@using TravelProject.Models.Sınıflar
@model List<Blog>

@{
    Layout = null;
}

<div class="con-bot">
    @foreach (var item in Model)
     {
        <div class="left">
            <div class="bot-left">
                <img src="@item.BlogImage" alt="" />
            </div>
            <div class="left-text">
                <h4>@item.Baslik</h4>
                <span class="doller">@item.Tarih.Year</span>
                <p>Açıklama Gelecek Loreminput Loreminput Loreminput Loreminput Loreminput </p>
                <p><a href="/Blog/BlogDetay/@item.ID"><span class="read">Devamını Oku >></span></a></p>
            </div>
        </div>
     }
    @Html.Action("Partial2","Ana")
    <div class="clear"></div>
</div>
