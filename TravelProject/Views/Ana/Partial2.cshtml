@using TravelProject.Models.Sınıflar
@model List<Blog>

@{
    Layout = null;
}

@foreach (var item in Model)
 {
    <div class="right">
        <div class="bot-left">
            <img src="@item.BlogImage" alt="" />
        </div>
        <div class="left-text">
            <h4>@item.Baslik</h4>
            <span class="doller">$924.99</span>
            <p>Lorem ipsum dolor sit amet, consectetuer adipiscing elit, sed diam nonummy nibh euismo</p>
            <p><a href="/Blog/BlogDetay/@item.ID"><span class="read">Devamını Oku >></span></a></p>
        </div>
    </div>
 }