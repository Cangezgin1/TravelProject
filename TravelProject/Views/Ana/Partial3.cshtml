@using TravelProject.Models.Sınıflar
@model List<Blog>

@{
    Layout = null;
}

<div class="padding">
        <h4>En Popüler 10 Blog</h4>
        @{ int i = 0; }
        @foreach (var item in Model)
         {
            <ul class="list list-pad">
                <li class="list-bg"><span>@{i++; }@i</span><a href="/Blog/BlogDetay/@item.ID">@item.Baslik</a></li>
            </ul>
         }
        <a href="/Blog" class="link1">Tüm Blogları Gör</a>
</div>